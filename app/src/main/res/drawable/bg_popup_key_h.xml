<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="#FFFF" />
            <solid android:color="#8000" />
        </shape>
    </item>
    <item
        android:bottom="2sp"
        android:left="2sp"
        android:right="2sp"
        android:top="2sp">
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="#8FFF" />
        </shape>
    </item>
</layer-list>
